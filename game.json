{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/main.js",
	"moduleMainScripts": {
		"@akashic-extension/akashic-hover-plugin": "node_modules/@akashic-extension/akashic-hover-plugin/lib/index.js",
		"@akashic-extension/resolve-player-info": "node_modules/@akashic-extension/resolve-player-info/lib/index.js",
		"@akashic-extension/akashic-label": "node_modules/@akashic-extension/akashic-label/lib/index.js"
	},
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"multi_admission"
			]
		},
		"external": {
			"coeLimited": "0",
			"atsumaru": "0"
		}
	},
	"globalScripts": [
		"node_modules/@akashic-extension/akashic-hover-plugin/lib/Converter.js",
		"node_modules/@akashic-extension/akashic-hover-plugin/lib/HoverPlugin.js",
		"node_modules/@akashic-extension/akashic-hover-plugin/lib/index.js",
		"node_modules/@akashic-extension/resolve-player-info/lib/FallbackDialog.js",
		"node_modules/@akashic-extension/resolve-player-info/lib/index.js",
		"node_modules/@akashic-extension/akashic-label/lib/DefaultRubyParser.js",
		"node_modules/@akashic-extension/akashic-label/lib/FragmentDrawInfo.js",
		"node_modules/@akashic-extension/akashic-label/lib/Label.js",
		"node_modules/@akashic-extension/akashic-label/lib/RubyParser.js",
		"node_modules/@akashic-extension/akashic-label/lib/index.js"
	],
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"500x500": {
			"type": "image",
			"path": "image/500x500.png",
			"width": 500,
			"height": 500
		},
		"player": {
			"type": "image",
			"path": "image/player.png",
			"width": 32,
			"height": 32
		},
		"shot": {
			"type": "image",
			"path": "image/shot.png",
			"width": 16,
			"height": 16
		},
		"usingStartParam": {
			"type": "script",
			"path": "script/usingStartParam.js",
			"global": true
		}
	}
}